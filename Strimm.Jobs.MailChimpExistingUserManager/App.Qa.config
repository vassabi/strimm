<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <!--Trigger job to run starting at every 10 minutes every day-->
    <add key="Cron.Schedule" value="0 0/60 * * * ?"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="IsActive" value="false"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="MAILCHIMP_LIST_ID" value="00ca0711b4" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  </appSettings>
</configuration>